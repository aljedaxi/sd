#!/usr/bin/env nix
#! nix shell nixpkgs#bash nixpkgs#csvkit --command bash

source ~/sd/xm/.env
branch=`curl -s -u $USERNAME:$TOKEN $OPEN_ISSUE_URL \
	| csvcut -c 2,1 \
	| csvformat -T \
	| tail -n +2 \
	| fzf \
	| sed 's/\t/-/' \
	| sed 's/ /-/g' \
	| sed 's/:/-/g'`
echo $branch
git checkout -b $branch
